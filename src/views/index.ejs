<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/navbar')  %>
<body>
    <%- include('./partials/head')  %>
    <div class="content">
        <% if (users.length > 0) { %>
            <% users.forEach(user => { %>
                <h3 class="title"><%= user.firstName %> <%= user.lastName %></h3>
                <p><%= user.email %></p>
            <% }) %>
        <% } else { %>
            <p>Não foram encontrados usuários cadastrados!</p>
        <% } %>
    </div>  
</body>
</html>